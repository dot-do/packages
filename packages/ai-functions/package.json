{
  "name": "@dot-do/ai-functions",
  "version": "0.1.0",
  "description": "Collection of AI-powered template literal functions for text generation, lists, research, and more",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "functions",
    "openai",
    "template-literals",
    "llm",
    "generative-ai",
    "typescript"
  ],
  "author": "dot-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/packages.git",
    "directory": "packages/ai-functions"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@google/genai": "^1.1.0",
    "@mendable/firecrawl-js": "^1.25.1",
    "ai": "^4.3.16",
    "dedent": "^1.6.0",
    "github-slugger": "^2.0.0",
    "gray-matter": "^4.0.3",
    "lru-cache": "^11.1.0",
    "object-hash": "^3.0.0",
    "p-queue": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.5",
    "yaml": "^2.8.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "^1.2.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
