{
  "name": "@dot-do/payload-experiments",
  "version": "0.1.0",
  "description": "A/B testing and experiment framework for PayloadCMS with statistical analysis",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./collections": {
      "import": "./dist/collections/index.mjs",
      "require": "./dist/collections/index.js",
      "types": "./dist/collections/index.d.ts"
    },
    "./lib": {
      "import": "./dist/lib/index.mjs",
      "require": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "payload": "^3.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "next": "^15.0.0"
  },
  "dependencies": {
    "js-cookie": "^3.0.5"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "keywords": [
    "payload",
    "payloadcms",
    "ab-testing",
    "experiments",
    "statistics",
    "conversion-tracking",
    "analytics",
    "multivariate-testing"
  ],
  "author": "dot-do",
  "license": "MIT"
}
