{
  "name": "@dot-do/ai-workflows",
  "version": "0.1.0",
  "description": "Workflow management and orchestration using AI-powered functions with Ink CLI",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "workflows",
    "orchestration",
    "ink",
    "cli",
    "automation",
    "typescript"
  ],
  "author": "dot-do",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/packages.git",
    "directory": "packages/ai-workflows"
  },
  "dependencies": {
    "@dot-do/ai-functions": "workspace:*",
    "react": "^18.2.0",
    "ink": "^4.4.1",
    "esbuild": "^0.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
